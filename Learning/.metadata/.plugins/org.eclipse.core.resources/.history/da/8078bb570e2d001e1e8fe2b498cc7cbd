
public class Inbuild {
	
	public static void main(String args [])
	{
		String s1 = "javaclass";
		System.out.println(s1.length());
		
		System.out.println(String.format(s1));
		
		String s2 = String.format("%s", s1);
		String s3 = String.format("|%+10d|", 101); //Altering string for altering between % and d
		String s4 = String.format("%f", 102.00);
		String s5 = String.format("%c", 'h');
		
		System.out.println(s2);
		System.out.println(s3);
		System.out.println(s4);
		System.out.println(s5);
		
		String a1 = "abcdefg";
		byte b1[] = a1.getBytes();
		
		for (int i =0; i<b1.length;i++)
		{
			System.out.println(b1[i]);
		}
		
		
		
		
		
		
		
		
		
	}

}
