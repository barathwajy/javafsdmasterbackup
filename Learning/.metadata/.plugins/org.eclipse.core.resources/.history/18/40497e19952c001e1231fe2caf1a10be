		for(int i=0;i<c;i++)
		{
			arr1k[i]=sc.nextInt();
			arr1s[i]=arr1k[i];
		}
		
		for(int i=0;i<c;i++)
		{
			arr2k[i]=sc.nextInt();
		}
		Arrays.sort(arr1s);
		
		/*
