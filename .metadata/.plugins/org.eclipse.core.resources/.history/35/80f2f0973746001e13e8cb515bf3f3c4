package junit_test_demo;

import org.junit.jupiter.api.Test;

public class Nestedemo {
	
	@Nestedemo
	public class test1{
		@Test
		public void demo1()
		{
			System.out.println("Test -1");
		}
		@Test
		public void demo2()
		{
			System.out.println("Test -2");
		}
	}
	
	@Test
	public void demo3()
	{
		System.out.println("Test -3");
	}
	@Test
	public void demo4()
	{
		System.out.println("Test -4");
	}

}
