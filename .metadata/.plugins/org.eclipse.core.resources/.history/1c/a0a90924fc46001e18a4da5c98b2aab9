package testing_d;

import org.junit.jupiter.params.ParameterizedTest;
import org.junit.jupiter.params.provider.ValueSource;

/*
 * Value Source
 * Argument source
 * enum source
 * method source
 */
public class Parameterdemos {
	
	@ParameterizedTest (name="(index)-run the arrray from zero={0}")
	@ValueSource(ints= {9,7,6,9})
	
	public void assum(int args)
	{
		System.out.println(args);	
	}
	
	@ParameterizedTest (name="(index)-run the arrray from zero={0}")
	@ValueSource(strings= {"9","7","6","9"})
	
	public void assum1(String args)
	{
		System.out.println(args);	
	}

	
	
	
	enum name{
		"aarthi","banu","kavi","moni","vani";
	}
	
	
	
}
